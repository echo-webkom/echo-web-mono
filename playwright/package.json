{
  "name": "@echo-webkom/playwright",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test:e2e": "pnpm with-env playwright test",
    "test:e2e:ui": "pnpm with-env playwright test --ui-port 5555",
    "test:install": "playwright install --with-deps",
    "test:report": "playwright show-report",
    "lint": "eslint tests",
    "lint:fix": "eslint tests --fix",
    "clean": "rm -rf node_modules playwright-report test-results .turbo",
    "with-env": "dotenv -e ../.env --",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@playwright/test": "1.58.0",
    "@types/node": "22.19.3",
    "dotenv-cli": "11.0.0",
    "eslint": "9.39.2",
    "globals": "^17.1.0",
    "postgres": "3.4.8",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "dependencies": {
    "jose": "^6.1.3"
  }
}
