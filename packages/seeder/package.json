{
  "name": "@echo-webkom/seeder",
  "version": "0.1.0",
  "main": "./src/index.ts",
  "scripts": {
    "seed:start": "pnpm with-env tsx ./src/index.ts",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint src",
    "lint:fix": "eslint lint --fix",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "devDependencies": {
    "@echo-webkom/tsconfig": "workspace:*",
    "@types/node": "20.16.1",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.0",
    "eslint-config-webkom": "workspace:*",
    "tsx": "4.17.0",
    "typescript": "5.5.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "webkom/base"
    ],
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "rules": {
          "no-console": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@echo-webkom/db": "workspace:*",
    "@echo-webkom/lib": "workspace:*",
    "@echo-webkom/sanity": "workspace:^",
    "@sanity/client": "6.21.3",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "drizzle-orm": "0.33.0",
    "groq": "3.54.0"
  }
}
