{
  "name": "@echo-webkom/seeder",
  "version": "0.1.0",
  "main": "./src/index.ts",
  "scripts": {
    "seed:start": "pnpm with-env tsx ./src/index.ts",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint src",
    "lint:fix": "eslint lint --fix",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "devDependencies": {
    "@types/node": "20.19.0",
    "dotenv-cli": "8.0.0",
    "eslint": "8.57.1",
    "eslint-config-webkom": "workspace:*",
    "tsx": "4.20.2",
    "typescript": "5.6.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "webkom/base"
    ],
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "rules": {
          "no-console": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@echo-webkom/db": "workspace:*",
    "@echo-webkom/lib": "workspace:*",
    "@echo-webkom/sanity": "workspace:^",
    "@faker-js/faker": "9.8.0",
    "@sanity/client": "7.6.0",
    "chalk": "5.4.1",
    "commander": "14.0.0",
    "drizzle-orm": "0.44.2",
    "groq": "3.91.0"
  }
}
