{
  "name": "@echo-webkom/db",
  "version": "0.1.0",
  "exports": {
    ".": "./src/index.ts",
    "./schemas": "./src/schemas/index.ts",
    "./utils": "./src/utils.ts",
    "./create": "./src/create.ts",
    "./serverless": "./src/serverless.ts",
    "./error": "./src/error.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:migrate": "pnpm with-env tsx ./drizzle/migrate.ts",
    "db:check": "pnpm with-env drizzle-kit check && tsx ./drizzle/check.ts",
    "kit:up": "drizzle-kit up",
    "dev": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "drizzle-orm": "0.44.4",
    "drizzle-zod": "0.5.1",
    "nanoid": "5.1.5",
    "pg": "8.16.0",
    "postgres": "3.4.7",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "20.19.4",
    "@types/pg": "8.15.4",
    "dotenv-cli": "8.0.0",
    "drizzle-kit": "0.31.4",
    "eslint": "9.9.0",
    "eslint-config-webkom": "workspace:*",
    "globals": "^15.15.0",
    "tsx": "4.20.3",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.0.0"
  }
}
