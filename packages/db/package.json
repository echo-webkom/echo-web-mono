{
  "name": "@echo-webkom/db",
  "version": "0.1.0",
  "exports": {
    ".": "./src/index.ts",
    "./schemas": "./src/schemas/index.ts",
    "./utils": "./src/utils.ts",
    "./create": "./src/create.ts",
    "./serverless": "./src/serverless.ts",
    "./error": "./src/error.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint lint --fix",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:migrate": "pnpm with-env tsx ./drizzle/migrate.ts",
    "db:check": "pnpm with-env drizzle-kit check && tsx ./drizzle/check.ts",
    "kit:up": "drizzle-kit up",
    "dev": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "drizzle-orm": "0.44.2",
    "drizzle-zod": "0.5.1",
    "nanoid": "5.1.5",
    "next-auth": "4.24.7",
    "pg": "8.16.0",
    "postgres": "3.4.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "22.15.29",
    "@types/pg": "8.15.2",
    "dotenv-cli": "8.0.0",
    "drizzle-kit": "0.31.1",
    "eslint-config-webkom": "workspace:*",
    "tsx": "4.19.4",
    "typescript": "5.6.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "webkom/base"
    ],
    "ignorePatterns": [
      "scripts"
    ]
  }
}
