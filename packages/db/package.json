{
  "name": "@echo-webkom/db",
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./schemas": "./schemas"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint lint --fix",
    "db:generate": "pnpm with-env drizzle-kit generate:pg",
    "db:migrate": "pnpm with-env tsx ./drizzle/migrate",
    "db:seed": "pnpm with-env tsx ./drizzle/seed",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "dev": "pnpm with-env drizzle-kit studio --port 4000",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@auth/core": "^0.17.0",
    "drizzle-orm": "^0.28.6",
    "nanoid": "^5.0.2",
    "postgres": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/pg": "^8.10.7",
    "dotenv-cli": "7.3.0",
    "drizzle-kit": "^0.19.13",
    "eslint-config-webkom": "workspace:*",
    "pg": "^8.11.3",
    "tsx": "^3.14.0",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "webkom/base"
    ]
  }
}
