{
  "name": "@echo-webkom/db",
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./schemas": "./schemas"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint lint --fix",
    "db:generate": "pnpm with-env drizzle-kit generate:pg",
    "db:migrate": "pnpm with-env tsx ./drizzle/migrate.ts",
    "db:seed": "pnpm with-env tsx ./drizzle/seed.ts",
    "db:check": "pnpm with-env tsx ./drizzle/check.ts",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "dev": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "drizzle-orm": "0.29.1",
    "drizzle-zod": "0.5.1",
    "nanoid": "5.0.3",
    "next-auth": "4.24.5",
    "pg": "8.11.3",
    "postgres": "3.4.3"
  },
  "devDependencies": {
    "@types/node": "18.18.14",
    "@types/pg": "8.10.9",
    "dotenv-cli": "7.3.0",
    "drizzle-kit": "0.20.6",
    "eslint-config-webkom": "workspace:*",
    "tsx": "4.6.1",
    "typescript": "5.3.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "webkom/base"
    ]
  }
}
