{
  "name": "@echo-webkom/db",
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./enums": "./enums.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "pnpm with-env prisma generate --schema=./prisma/schema.prisma && tsx ./prisma/post-generate.ts",
    "db:migrate": "pnpm with-env prisma migrate dev --schema ./prisma/schema.prisma",
    "db:deploy": "pnpm with-env prisma migrate deploy --schema ./prisma/schema.prisma",
    "docker:up": "docker-compose up -d",
    "lint": "eslint .",
    "lint:fix": "eslint lint --fix",
    "docker:down": "docker-compose down",
    "dev": "pnpm with-env prisma studio --port 4000 --browser none",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "dotenv-cli": "7.3.0",
    "eslint-config-webkom": "workspace:*",
    "prisma": "^5.2.0",
    "prisma-kysely": "^1.7.0",
    "tsx": "^3.12.8",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "webkom/base"
    ]
  }
}
