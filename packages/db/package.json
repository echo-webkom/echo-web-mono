{
  "name": "@echo-webkom/db",
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./schemas": "./schemas"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint lint --fix",
    "db:generate": "pnpm with-env drizzle-kit generate:pg",
    "db:migrate": "pnpm with-env tsx ./drizzle/migrate.ts",
    "db:seed": "pnpm with-env tsx ./drizzle/seed.ts",
    "db:check": "pnpm with-env tsx ./drizzle/check.ts",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "dev": "pnpm with-env drizzle-kit studio --port 4000",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "drizzle-orm": "^0.29.0",
    "drizzle-zod": "^0.5.1",
    "nanoid": "^5.0.3",
    "next-auth": "^4.24.4",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/pg": "^8.10.9",
    "dotenv-cli": "7.3.0",
    "drizzle-kit": "^0.19.13",
    "eslint-config-webkom": "workspace:*",
    "tsx": "^4.0.0",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "webkom/base"
    ]
  }
}
