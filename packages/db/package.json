{
  "name": "@echo-webkom/db",
  "version": "0.1.0",
  "exports": {
    ".": "./src/index.ts",
    "./schemas": "./src/schemas/index.ts",
    "./utils": "./src/utils.ts",
    "./create": "./src/create.ts",
    "./serverless": "./src/serverless.ts",
    "./error": "./src/error.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:migrate": "pnpm with-env tsx ./drizzle/migrate.ts",
    "db:check": "pnpm with-env drizzle-kit check && tsx ./drizzle/check.ts",
    "db:reset": "pnpm with-env tsx ./scripts/reset.ts",
    "kit:up": "drizzle-kit up",
    "dev": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "nanoid": "5.1.6",
    "pg": "8.17.2",
    "postgres": "3.4.8",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "22.19.7",
    "@types/pg": "8.16.0",
    "dotenv-cli": "11.0.0",
    "drizzle-kit": "0.31.8",
    "drizzle-seed": "^0.3.1",
    "eslint": "9.39.2",
    "globals": "^17.1.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
