{
  "name": "@echo-webkom/db",
  "version": "0.1.0",
  "types": "./types.ts",
  "files": [
    "client.ts",
    "types.ts"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "pnpm with-env prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema ./prisma/schema.prisma",
    "db:deploy": "prisma migrate deploy --schema ./prisma/schema.prisma",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "dev": "pnpm with-env prisma studio --port 4000 --browser none",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0"
  },
  "devDependencies": {
    "dotenv-cli": "7.2.1",
    "prisma": "^4.13.0",
    "typescript": "^5.0.4"
  }
}
