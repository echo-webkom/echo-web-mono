    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "eslint": "^8.50.0",
    "eslint-config-webkom": "workspace:*",
    "pg-native": "^3.0.1",
    "postgres": "^3.4.0",
    "redis": "^4.6.10",
    "server-only": "^0.0.1",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "zod": "^3.22.2"
