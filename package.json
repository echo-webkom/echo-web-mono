{
  "name": "@echo-webkom/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm run with-env next build",
    "dev": "pnpm run with-env next dev",
    "lint": "SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm run with-env next start",
    "clean": "rm -rf .next .turbo node_modules",
    "test:unit": "env TZ=UTC-1 vitest",
    "analyze": "ANALYZE=true pnpm build",
    "format": "prettier --write \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:migrate": "pnpm with-env tsx ./drizzle/migrate.ts",
    "db:check": "pnpm with-env drizzle-kit check && tsx ./drizzle/check.ts",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:down-volumes": "docker compose down --volumes",
    "test:e2e": "TESTING=\"true\" pnpm with-env playwright test",
    "test:e2e:ui": "TESTING=\"true\" pnpm with-env playwright test --ui-port 5555",
    "test:install": "playwright install --with-deps",
    "test:report": "playwright show-report",
    "with-env": "dotenv -e ../../.env --",
    "seed": "pnpm with-env tsx ./seeder/index.ts",
    "typecheck": "tsc --noEmit",
    "extract": "sanity schema extract --enforce-required-fields",
    "typegen": "sanity typegen generate"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@json2csv/plainjs": "7.0.6",
    "@radix-ui/react-alert-dialog": "1.1.1",
    "@radix-ui/react-avatar": "1.1.0",
    "@radix-ui/react-checkbox": "1.1.1",
    "@radix-ui/react-dialog": "1.1.1",
    "@radix-ui/react-dropdown-menu": "2.1.1",
    "@radix-ui/react-hover-card": "1.1.1",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-popover": "1.1.1",
    "@radix-ui/react-separator": "1.1.0",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-switch": "1.1.0",
    "@radix-ui/react-toast": "1.2.1",
    "@react-email/components": "^0.0.25",
    "@sanity/client": "6.21.3",
    "@sanity/icons": "3.3.1",
    "@sanity/image-url": "1.0.2",
    "@sanity/ui": "2.8.8",
    "@sanity/vision": "3.56.0",
    "@t3-oss/env-nextjs": "0.11.1",
    "@types/remove-markdown": "0.3.4",
    "@vercel/analytics": "1.3.1",
    "@vercel/speed-insights": "1.0.12",
    "calendar-link": "2.7.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "drizzle-orm": "0.33.0",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "11.3.29",
    "groq": "3.56.0",
    "ics": "3.7.6",
    "nanoid": "5.0.7",
    "next": "14.2.6",
    "next-auth": "4.24.7",
    "next-sanity": "^9.4.7",
    "next-themes": "0.3.0",
    "nextjs-toploader": "1.6.12",
    "pg": "^8.12.0",
    "react": "18.3.1",
    "react-confetti": "6.1.0",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "react-icons": "5.3.0",
    "react-markdown": "9.0.1",
    "react-use-websocket": "4.8.1",
    "remark-gfm": "4.0.0",
    "remove-markdown": "0.5.2",
    "resend": "^4.0.0",
    "sanity": "^3.57.0",
    "sanity-plugin-markdown": "^4.1.2",
    "sanity-plugin-media": "^2.3.2",
    "sharp": "0.33.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "2.5.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
