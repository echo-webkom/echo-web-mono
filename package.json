{
  "name": "echo-web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.14.3",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=web",
    "web:dev": "turbo run dev --filter=web",
    "cms:dev": "turbo run dev --filter=cms",
    "db:dev": "turbo run dev --filter=db",
    "start": "turbo run start",
    "setup": "pnpm install && pnpm db:up && pnpm db:migrate",
    "clean": "rm -rf node_modules",
    "clean-workspaces": "turbo run clean",
    "db:generate": "pnpm --filter=db db:generate",
    "db:migrate": "pnpm --filter=db db:migrate",
    "db:seed": "pnpm --filter=db db:seed",
    "db:check": "pnpm --filter=db db:check",
    "db:up": "pnpm --filter=db db:up",
    "db:down": "pnpm --filter=db db:down",
    "db:remove": "pnpm db:down && rm -rf packages/db/postgres-data",
    "db:setup": "pnpm db:remove && pnpm db:up && pnpm sleep2s && pnpm db:migrate && pnpm db:seed",
    "sleep2s": "node -e \"setTimeout(() => process.exit(0), 2000)\"",
    "email:preview": "pnpm --filter=email preview",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "typecheck": "turbo run typecheck",
    "test:unit": "turbo run test:unit",
    "test:e2e": "turbo run test:e2e --concurrency=11",
    "test:e2e:ui": "turbo run test:e2e:ui"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/prettier": "2.7.3",
    "prettier": "3.2.4",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.11",
    "start-server-and-test": "2.0.3",
    "turbo": "1.11.3",
    "typescript": "5.3.3"
  }
}
