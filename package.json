{
  "name": "echo-web",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "web:dev": "turbo run dev --filter=web",
    "cms:dev": "turbo run dev --filter=cms",
    "db:dev": "turbo run dev --filter=db",
    "db:start": "pnpm --filter=db db:start",
    "start": "turbo run start",
    "setup": "pnpm install && pnpm db:up && pnpm db:migrate",
    "clean": "rm -rf node_modules",
    "clean-workspaces": "turbo run clean",
    "db:generate": "pnpm --filter=db db:generate",
    "db:migrate": "pnpm --filter=db db:migrate",
    "db:check": "pnpm --filter=db db:check",
    "seed": "pnpm --filter=seeder seed:start",
    "email:preview": "pnpm --filter=email preview",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,cjs,json,md}\" ",
    "typecheck": "turbo run typecheck",
    "test:unit": "turbo run test:unit",
    "test:e2e": "turbo run test:e2e --concurrency=11",
    "test:e2e:ui": "turbo run test:e2e:ui"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "prettier-plugin-tailwindcss": "0.6.6",
    "turbo": "2.0.14",
    "typescript": "5.5.4"
  }
}
