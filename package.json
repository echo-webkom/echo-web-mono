{
  "name": "echo-web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "web:dev": "turbo run dev --filter=web",
    "docs:dev": "turbo run dev --filter=docs",
    "cms:dev": "turbo run dev --filter=cms",
    "db:dev": "turbo run dev --filter=db",
    "start": "turbo run start",
    "clean": "rm -rf node_modules",
    "clean-workspaces": "turbo run clean",
    "db:generate": "turbo run db:generate",
    "db:migrate": "pnpm --filter=db db:migrate",
    "db:deploy": "turbo run db:deploy",
    "docker:up": "pnpm --filter=db docker:up",
    "docker:down": "pnpm --filter=db docker:down",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "type-check": "turbo run type-check",
    "test:e2e": "pnpm --filter=playwright test",
    "test:e2e:ui": "pnpm --filter=playwright ui",
    "postinstall": "pnpm run db:generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@types/prettier": "^2.7.3",
    "husky": "8.0.3",
    "prettier": "3.0.2",
    "prettier-plugin-organize-imports": "3.2.3",
    "prettier-plugin-tailwindcss": "0.5.3",
    "start-server-and-test": "^2.0.0",
    "turbo": "1.10.12",
    "typescript": "5.1.6"
  }
}
