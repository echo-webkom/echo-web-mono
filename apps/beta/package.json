{
	"name": "@echo-webkom/beta",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "pnpm with-env vite dev",
		"build": "pnpm with-env vite build",
		"preview": "vite preview",
		"check": "pnpm with-env svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "pnpm with-env svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"with-env": "dotenv -e ../../.env --",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@echo-webkom/cms": "workspace:*",
		"@sveltejs/adapter-node": "^5.2.10",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"dotenv-cli": "7.4.2",
		"drizzle-kit": "^0.22.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@echo-webkom/db": "workspace:*",
		"@echo-webkom/sanity": "workspace:*",
		"@node-rs/argon2": "^1.1.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sanity/client": "6.22.5",
		"arctic": "^2.3.1",
		"class-variance-authority": "0.7.0",
		"clsx": "2.1.1",
		"date-fns": "4.1.0",
		"drizzle-orm": "^0.33.0",
		"lucide-svelte": "^0.468.0",
		"marked": "^15.0.3",
		"nanoid": "5.0.9",
		"remove-markdown": "0.5.5",
		"tailwind-merge": "2.5.4"
	}
}
