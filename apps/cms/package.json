{
  "name": "@echo-webkom/cms",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./types": "./sanity.types.ts"
  },
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "extract": "sanity schema extract --enforce-required-fields",
    "typegen": "sanity typegen generate",
    "clean": "rm -rf node_modules .sanity dist",
    "deploy-graphql": "sanity graphql deploy",
    "lint": "eslint '**/*.{js,ts,jsx,tsx}'",
    "lint:fix": "eslint '**/*.{js,ts,jsx,tsx}' --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@echo-webkom/lib": "workspace:*",
    "@sanity/client": "6.27.2",
    "@sanity/icons": "3.5.7",
    "@sanity/ui": "2.11.7",
    "@sanity/vision": "3.71.2",
    "nanoid": "5.0.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "sanity": "3.71.2",
    "sanity-plugin-markdown": "4.1.2",
    "sanity-plugin-media": "2.3.2",
    "sanity-plugin-singleton-tools": "^1.1.0",
    "slugify": "1.6.6",
    "styled-components": "6.1.13"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "4.0.0",
    "@types/react": "18.3.12",
    "@types/styled-components": "5.1.34",
    "dotenv": "16.4.5",
    "eslint": "8.57.1",
    "eslint-config-webkom": "workspace:*",
    "typescript": "5.6.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "webkom/base",
      "webkom/react"
    ],
    "ignorePatterns": [
      "migrations"
    ]
  }
}
