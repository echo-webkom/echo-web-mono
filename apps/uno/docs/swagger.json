{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample API with Chi and Swaggo.",
        "title": "Chi Example API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/happenings/{id}": {
            "get": {
                "description": "Returns the happening with the specified ID",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "happenings"
                ],
                "summary": "Get happening by ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Happening ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        }
    }
}