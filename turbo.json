{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        ".sanity/**",
        "dist/**",
        ".svelte-kit/**",
        "build/**",
        ".vitepress/dist/**"
      ]
    },
    "start": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "test:e2e": {
      "persistent": true,
      "cache": false
    },
    "test:e2e:ui": {
      "persistent": true,
      "cache": false
    },
    "email:preview": {
      "persistent": true,
      "cache": false
    },
    "test:unit": {},
    "lint": {},
    "lint:fix": {
      "cache": false
    },
    "typecheck": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "format": {
      "cache": false
    },
    "format:check": {
      "cache": false
    }
  },
  "globalDependencies": [".env"],
  "globalPassThroughEnv": ["PLAYWRIGHT_*"],
  "globalEnv": [
    "CI",
    "NODE_ENV",
    "VERCEL_ENV",
    "DATABASE_URL",
    "FEIDE_CLIENT_ID",
    "FEIDE_CLIENT_SECRET",
    "ADMIN_KEY",
    "NEXT_PUBLIC_SANITY_DATASET",
    "VERCEL_URL",
    "PORT",
    "SANITY_TOKEN",
    "RESEND_API_KEY",
    "ECHOGRAM_API_KEY",
    "NEXT_PUBLIC_SANITY_PROJECT_ID",
    "NEXT_PUBLIC_ECHOGRAM_URL",
    "ECHOGRAM_API_KEY",
    "DATABASE_LOG",
    "NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA",
    "NEXT_PUBLIC_API_URL",
    "API_PORT",
    "AOC_SESSION_COOKIE",
    "AUTH_SECRET"
  ]
}
