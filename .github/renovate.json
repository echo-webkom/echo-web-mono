{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":semanticCommitScopeDisabled"],
  "commitMessagePrefix": "renovate:",
  "commitMessageAction": "update",
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "matchPackagePatterns": ["^@echo-webkom/"],
      "enabled": false
    }
  ],
  "prConcurrentLimit": 5,
  "stabilityDays": 3
}
